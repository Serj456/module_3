calls = 0
def count_calls():
    global calls
    calls = calls + 1
def string_info(string):
    count_calls()
    info = (len(string), string.upper(), string.lower())
    return info
def is_contains(string,list_to_research):
    count_calls()
    string = string.upper()
    list_to_research = [x.upper() for x in list_to_research]
    if string in list_to_research:
        return True
    else:
        return False



print(string_info('capybara'))
print(string_info('VeloSiped'))
print(string_info('Python eto slojno'))
print(is_contains('Urban', ['ban', 'BaNaN', 'urBAN']))
print(is_contains('cycle', ['recycling', 'cyclic']))
print(is_contains('Сережа', ['Ананас', 'cyclic', 'cережа']))
print(is_contains('Module', ['ban', 'moDUle', 'learn','65']))
print(calls)
